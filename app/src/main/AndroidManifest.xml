<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clantikiti">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/concertone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clantikiti">
        <activity
            android:name=".UI.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".UI.CreateAccountActivity"
            android:exported="true" >

        </activity>
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.SavedEventListActivity"
            android:exported="true" />
        <activity
            android:name=".UI.event_location"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.clantikiti.UI.MainActivity" />
        </activity>
        <activity
            android:name=".UI.EventDetailActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.clantikiti.UI.MainActivity" />
        </activity>
        <activity
            android:name=".UI.EventListActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.clantikiti.UI.MainActivity" />
        </activity>
        <activity
            android:name=".UI.MainActivity"
            android:exported="true">
        </activity>

    </application>

</manifest>